{
  "name": "ti4-lrr",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "lunr": "^2.3.9",
    "node-sass": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^7.1.1",
    "react-router-dom": "6",
    "react-scripts": "4.0.3",
    "slugify": "^1.6.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "PORT=3010 react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "prebuild": "npm run index-search",
    "build": "npm run build-css && react-scripts build && react-snapshot",
    "deploy": "aws --profile nic-cli s3 sync build s3://com.basicallyfine.ti4.lrr --exclude '*DS_Store'",
    "index-search": "babel-node --presets babel-preset-es2015 -- workflow/index-content-search.js",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^5.1.3",
    "bootswatch": "^5.1.3",
    "markdown-to-txt": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "react-bootstrap": "^2.0.3",
    "react-snapshot": "^1.3.0"
  },
  "reactSnapshot": {
    "snapshotDelay": 200
  }
}
